#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/AllCodes/NodeSerialPort/node_modules/.pnpm/@serialport+list@12.0.0/node_modules/@serialport/list/dist/node_modules:/mnt/d/AllCodes/NodeSerialPort/node_modules/.pnpm/@serialport+list@12.0.0/node_modules/@serialport/list/node_modules:/mnt/d/AllCodes/NodeSerialPort/node_modules/.pnpm/@serialport+list@12.0.0/node_modules/@serialport/node_modules:/mnt/d/AllCodes/NodeSerialPort/node_modules/.pnpm/@serialport+list@12.0.0/node_modules:/mnt/d/AllCodes/NodeSerialPort/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/AllCodes/NodeSerialPort/node_modules/.pnpm/@serialport+list@12.0.0/node_modules/@serialport/list/dist/node_modules:/mnt/d/AllCodes/NodeSerialPort/node_modules/.pnpm/@serialport+list@12.0.0/node_modules/@serialport/list/node_modules:/mnt/d/AllCodes/NodeSerialPort/node_modules/.pnpm/@serialport+list@12.0.0/node_modules/@serialport/node_modules:/mnt/d/AllCodes/NodeSerialPort/node_modules/.pnpm/@serialport+list@12.0.0/node_modules:/mnt/d/AllCodes/NodeSerialPort/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@serialport/list/dist/index.js" "$@"
else
  exec node  "$basedir/../@serialport/list/dist/index.js" "$@"
fi
